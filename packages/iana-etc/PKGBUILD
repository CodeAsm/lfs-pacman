pkgname=iana-etc
pkgver=20240125
pkgrel=1
pkgdesc="/etc/protocols and /etc/services provided by IANA"
arch=('x86_64')
url="http://www.iana.org/protocols"
license=('custom:none')
source=(${pkgname}-${pkgver}.tar.gz)

package() {
    cd "${pkgname}-${pkgver}"
    mkdir -p ${pkgdir}/etc
    cp services protocols ${pkgdir}/etc
}
